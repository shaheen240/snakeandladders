#include <iostream>
using namespace std;
int main (){
    int i, n , count;
    
    cout<<"enter any positive no" <<endl;
    cin>>n;
    
    for(i=1 ; i<=n ; i++){
        if(n%i==0){
            count++;
        }
    }
    if(count==2){
        cout<<n <<" is prime no ";
    }
    
    else {
        cout<<n <<"is not a prime no";
    }
    
    return 0;
    
}
